---
export interface Props {
	link: string;
}

const {link} = Astro.props;
---

<div>
    <a href={link} target="_blank">SATIN AL</a>
    <span>cosmetica</span>
</div>

<style>
    a {
        font-size: 2.5rem;
        width: auto;
        padding: 6px 14px;
        background: rgb(var(--accent));
        color: #fff;
        border-radius: 10px;
        text-decoration: none;
        margin: 8px 0;
    }
</style>